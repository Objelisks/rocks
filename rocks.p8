pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
save("rocks")

sx = 4.5
sy = 4.5
worker_speed = 1


-- tinylist(tm) methodology
tasks = {}
avail_tasks = {}
idlers = {}
workers = {}

function _init()
	add(workers, new_actor(4+rnd(2), 4+rnd(4)))
	add(workers, new_actor(4+rnd(2), 4+rnd(4)))
	add(workers, new_actor(4+rnd(2), 4+rnd(4)))
	for w in all(workers) do
		add(idlers, w)
	end
end

function _update()
	if(btnp(0)) sx -= 1
	if(btnp(1)) sx += 1
	if(btnp(2)) sy -= 1
	if(btnp(3)) sy += 1

	if(btnp(4)) then
		local task = {i=id(), x=sx, y=sy, t=30}
		add(tasks, task)
		add(avail_tasks, task)
	end
	
	for task in all(tasks) do


		-- task finished
		if(task.t <= 0) then

			-- remove task
			del(tasks, task)
			del(avail_tasks, task)
			if(task.w) then
				task.w.t = nil
				task.w = nil
			end

			-- do task action
			autotile(task.x+0.5, task.y+0.5, false)
		end
	end
		
	for worker in all(workers) do

		-- move along path if have
		if(worker.p and all(worker.p)()) then
			local next = all(worker.p)()

			-- move to next waypoint
			local d = dist(worker.x, worker.y,
						next.x, next.y)
			worker.x += (next.x - worker.x) / d * worker_speed / 30.0
			worker.y += (next.y - worker.y) / d * worker_speed / 30.0

			-- update next waypoint
			if(d < 1) then
				del(worker.p, next)
			end
		end

		if(worker.t) then
			local task_dist = dist(worker.x, worker.y, worker.t.x+0.5, worker.t.y+0.5)
			log(task_dist)

			-- work on task
			if(worker.t and task_dist < 1) then
				worker.t.t -= 1
			end

			-- if we have a task and no path to task
			if(worker.t and not all(worker.p)()) then
				-- pathfind to middle of task
				add(worker.p, {x=worker.t.x+0.5, y=worker.t.y+0.5})
			end
		else
			if(all(avail_tasks)()) then
				-- get task
				worker.t = all(avail_tasks)()
				del(avail_tasks, worker.t)
				worker.t.w = worker
			end
		end
	end

end

function _draw()
	cls()
	mapdraw(0,0,0,0,128,128)
	
	for task in all(tasks) do
		spr(3, task.x*8, task.y*8)
	end
	
	for worker in all(workers) do
		spr(6, worker.x*8, worker.y*8)
	end

	spr(2, sx*8, sy*8)

	camera()

	flushlog()
end

function autotile(x, y, solid)
	updatetile(x-1, y-1, 8, solid)
	updatetile(x-1, y, 2, solid)
	updatetile(x, y-1, 4, solid)
	updatetile(x, y, 1, solid)
end

local tiles_offset = 16
function updatetile(tx, ty, dir, solid)
	if(tx < 0 or ty < 0 or tx > 128 or ty > 128) return
	
	local current = mget(tx, ty)-tiles_offset
	local newval = solid and bor(current, dir) or band(current,bnot(dir))
	mset(tx, ty, newval+tiles_offset)
end

function new_actor(x, y)
	return {x=x, y=y, r=0, p={}}
end

function dist(x1, y1, x2, y2)
	return abs(x2-x1)+abs(y2-y1)
end

do
local i = 0
function id()
	i += 1
	return i
end
end

do -- log, flushlog
	local logs = {}
	function log(str)
		print(str,4,8,8)
		add(logs, str)
	end
	function flushlog()
		local i = 0
		for str in all(logs) do
			print(str,4,4+8*i)
			i += 1
		end
		logs = {}
	end
end











